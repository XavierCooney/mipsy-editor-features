{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "out",
		"lib": [
			"ES2021",
			"DOM"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"resolveJsonModule": true
	}
}
